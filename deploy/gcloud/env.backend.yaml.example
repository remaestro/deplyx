APP_NAME: deplyx
ENV: production
JWT_ALGORITHM: HS256
JWT_EXPIRE_MINUTES: "60"
CORS_ALLOWED_ORIGINS: https://deplyx-frontend.example.com

# Neo4j Aura recommandé
NEO4J_URI: bolt+s://YOUR-AURA-ENDPOINT.databases.neo4j.io:7687
NEO4J_USERNAME: neo4j

# Postgres (Cloud SQL)
POSTGRES_DB: deplyx
POSTGRES_USER: deplyx
POSTGRES_PORT: "5432"

# Redis (Memorystore via private IP ou Redis managé compatible URL)
REDIS_URL: redis://10.0.0.10:6379/0
APPROVAL_TIMEOUT_HOURS: "48"
